{
  "name": "composemailer",
  "version": "1.0.0",
  "description": "A mail utility to parse a csv into a known group of templates and send the email",
  "main": "./src/index.ts",
  "scripts": {
    "start": "./node_modules/.bin/ts-node ./src/index.ts",
    "debugStart": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffisadams/composemailer.git"
  },
  "keywords": [
    "mailer",
    "nodemailer"
  ],
  "author": "Jeff Adams <jeffisadams@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "config": "1.24.0",
    "email-templates": "^2.5.4",
    "handlebars": "4.0.6",
    "less": "2.7.1",
    "moment": "2.17.1",
    "nodemailer": "^2.7.0",
    "papaparse": "^4.3.6",
    "ts-node": "^3.3.0"
  },
  "devDependencies": {
    "@types/config": "0.0.30",
    "@types/node": "^8.0.1",
    "@types/nodemailer": "1.3.32",
    "@types/papaparse": "^4.1.31",
    "typescript": "^2.6.1"
  },
  "bugs": {
    "url": "https://github.com/jeffisadams/composemailer/issues"
  },
  "homepage": "https://github.com/jeffisadams/composemailer#readme"
